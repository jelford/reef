<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel>
		<g:Label text="Upload Workloads" />
		<g:FormPanel ui:field="form">
			<g:HTMLPanel>
				<table>
					<tr>
						<td>Name</td><td>:</td><td><g:TextBox name="wkld_name"/></td>
					</tr>
					<tr>
						<td>File</td><td>:</td><td><g:FileUpload name="wkld_file"/></td>
					</tr>
					<tr>
						<td>Or Text</td><td>:</td><td><g:TextArea name="wkld_text"/></td>
					</tr>
				</table>
			</g:HTMLPanel>
		</g:FormPanel>
		<g:Button text="Upload Workload" ui:field="submit" />
		<g:ListBox ui:field="workloads" />
	</g:VerticalPanel>
</ui:UiBinder> 